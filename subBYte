`timescale 1ns / 1ps

module subBYte(data,sb);

input [127:0] data;
output [127:0] sb;

     RijndaelSBox ( .a(data[127:120]),.c(sb[127:120]) );
     RijndaelSBox ( .a(data[119:112]),.c(sb[119:112]) );
     RijndaelSBox ( .a(data[111:104]),.c(sb[111:104]) );
     RijndaelSBox ( .a(data[103:96]),.c(sb[103:96]) );
     
     RijndaelSBox ( .a(data[95:88]),.c(sb[95:88]) );
     RijndaelSBox ( .a(data[87:80]),.c(sb[87:80]) );
     RijndaelSBox ( .a(data[79:72]),.c(sb[79:72]) );
     RijndaelSBox ( .a(data[71:64]),.c(sb[71:64]) );
     
     RijndaelSBox ( .a(data[63:56]),.c(sb[63:56]) );
     RijndaelSBox ( .a(data[55:48]),.c(sb[55:48]) );
     RijndaelSBox (.a(data[47:40]),.c(sb[47:40]) );
     RijndaelSBox (.a(data[39:32]),.c(sb[39:32]) );
     
     RijndaelSBox (.a(data[31:24]),.c(sb[31:24]) );
     RijndaelSBox (.a(data[23:16]),.c(sb[23:16]) );
     RijndaelSBox (.a(data[15:8]),.c(sb[15:8]) );
     RijndaelSBox (.a(data[7:0]),.c(sb[7:0]) );
	  
endmodule
